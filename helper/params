source helper/defaults
source param/board/$TARGET
if test -v SETUP ; then 
	for S in $SETUP; do
		source param/setup/$S
	done
fi

test -v DEB_ARCH || DEB_ARCH=$ARCH
test -v BIN_ARCH || BIN_ARCH=$ARCH
if test $(dpkg --print-architecture) = $DEB_ARCH ; then
	CROSS_COMPILE=""
else
	test -v CROSS_COMPILE || CROSS_COMPILE=${BIN_ARCH}-linux-gnu-
fi
export ARCH CROSS_COMPILE
