# Raspi 32bit

ARCH=arm64
BIN_ARCH=aarch64
UBOOT_ARCH=arm
CROSS_COMPILE=aarch64-linux-gnu-

PART_OFFSET=1

P_BOOT=1
P_ROOT=2

BOOT_SIZE=${BOOT_SIZE:-$(expr 300 \* 1024 \* 2)}
ROOT_SIZE=${ROOT_SIZE:-$(expr 800 \* 1024 \* 2)}

BOOT_START=8192
ROOT_START=$(expr ${BOOT_START} + ${BOOT_SIZE} + 32)
DEVICE_SIZE=$(expr ${ROOT_START} + ${ROOT_SIZE} + 64) # workaround

